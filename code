  <script>
    function filterCards(filter) {
      const cards = document.querySelectorAll('.card');
      cards.forEach(card => {
        if (filter === 'all' || card.classList.contains(filter)) {
          card.classList.remove('hidden');
        } else {
          card.classList.add('hidden');
        }
      });
    }
  </script>
